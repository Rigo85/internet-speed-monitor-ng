{
	"name": "internet-speed-monitor-ng",
	"version": "1.0.0",
	"scripts": {
		"start-electron": "electron .",
		"ng": "ng",
		"start": "ng build --base-href ./ && npm run build-app-ts && npm run copy-static-assets && electron .",
		"package-mac": "electron-packager . internet-speed-monitor --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
		"package-win": "electron-packager . internet-speed-monitor --overwrite --asar --platform=win32 --arch=x64 --icon=public/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Internet speed monitor\"",
		"package-linux": "electron-packager . internet-speed-monitor --overwrite --asar --platform=linux --arch=x64 --icon=public/icon.png --prune=true --out=release-builds",
		"complete-package-linux": "npm run package-linux && ts-node --project electron-app-tsconfig.json copyStaticAssetsRelease.ts",
		"complete-package-win": "npm run package-win && ts-node --project electron-app-tsconfig.json copyStaticAssetsRelease.ts",
		"copy-static-assets": "ts-node --project electron-app-tsconfig.json copyStaticAssets.ts",
		"build": "ng build",
		"build-app-ts": "tsc -p electron-app-tsconfig.json",
		"watch": "ng build --watch --configuration development",
		"test": "ng test"
	},
	"main": "dist-electron-app/src-electron-app/main.js",
	"private": true,
	"author": "Rigoberto L. Salgado Reyes",
	"license": "AGPL-3.0",
	"engines": {
		"node": ">=22.0.0"
	},
	"dependencies": {
		"@angular/animations": "^18.0.0",
		"@angular/common": "^18.0.0",
		"@angular/compiler": "^18.0.0",
		"@angular/core": "^18.0.0",
		"@angular/forms": "^18.0.0",
		"@angular/platform-browser": "^18.0.0",
		"@angular/platform-browser-dynamic": "^18.0.0",
		"@angular/router": "^18.0.0",
		"angular-datatables": "^18.0.0",
		"bootstrap": "^5.3.3",
		"datatables.net": "^2.0.3",
		"datatables.net-bs5": "^2.0.3",
		"datatables.net-buttons": "^3.1.2",
		"datatables.net-buttons-dt": "^3.1.2",
		"dotenv": "^16.4.5",
		"electron-log": "^5.1.7",
		"jquery": "^3.6.0",
		"jszip": "^3.10.1",
		"rxjs": "~7.8.0",
		"sqlite3": "^5.1.7",
		"tslib": "^2.3.0",
		"url": "^0.11.4",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.0.6",
		"@angular/cli": "^18.0.6",
		"@angular/compiler-cli": "^18.0.0",
		"@electron/packager": "^18.3.6",
		"@types/jasmine": "~5.1.0",
		"@types/jquery": "^3.5.9",
		"@types/shelljs": "^0.8.15",
		"electron": "^32.0.1",
		"jasmine-core": "~5.1.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"shelljs": "^0.8.5",
		"ts-node": "^10.9.2",
		"typescript": "~5.4.2"
	}
}
